    </Style>

    <Style x:Key="TeslaToggle" x:Name="ButtonStyle" TargetType="{x:Type ToggleButton}">
        <Setter x:Name="ButtonStyle_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="ButtonStyle_ControlTemplate" TargetType="{x:Type ToggleButton}">
                    <Canvas Name="TeslaToggle_Canvas" Width="105" Height="50">
                        <Canvas.Resources>
                            <RectangleGeometry x:Key="clip0" Rect="0,0 , 90, 40"/>
                        </Canvas.Resources>
                        <Canvas Name="TeslaToggle_Rectangle_Canvas" Height="37" Width="55">
                            <Border Width="75" Height="39" CornerRadius="20" Name="TeslaToggle_Rectangle" Background="#DBDBDB" >

                            </Border>
                            <Canvas Name="g8" Height="41" Width="42">
                                <Border Canvas.Left="3.5" Canvas.Top="3" Width="33" Height="33" CornerRadius="20" x:Name="TeslaToggle_Light_BallSlide" Background="#ffffff">
                                    <Border.Effect>
                                        <DropShadowEffect ShadowDepth="1.2" BlurRadius="12" Color="#FFD4D4D4" Direction="357"/>
                                    </Border.Effect>
                                </Border>
                            </Canvas>
                        </Canvas>
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="ButtonStyleChecked_EventTrigger" RoutedEvent="Checked">
                            <BeginStoryboard x:Name="ButtonStyleChecked_BeginStoryBoard">
                                <Storyboard x:Name="ButtonStyleChecked_StoryBoard">
                                    <DoubleAnimation x:Name="ButtonSyleChecked_ThicknessAnimation" Storyboard.TargetName="TeslaToggle_Light_BallSlide"
                                                        Storyboard.TargetProperty="(Canvas.Left)"
                                                        To="39"
                                                        Duration="0:0:0.16"/>
                                    <ColorAnimation x:Name="ButtonStyleChecked_ColorAnimation" Storyboard.TargetName="TeslaToggle_Rectangle"
                                                    Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"
                                                    To="DodgerBlue"
                                                    Duration="0:0:0.16" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger x:Name="ButtonStyleUnchecked_EventTrigger" RoutedEvent="Unchecked">
                            <BeginStoryboard x:Name="ButtonStyleUnChecked_BrginStoryBoard">
                                <Storyboard x:Name="ButtonStyleUnChecked_StoryBoard">
                                    <DoubleAnimation x:Name="ButtonSyleUnChecked_ThicknessAnimation" Storyboard.TargetName="TeslaToggle_Light_BallSlide"
                                                        Storyboard.TargetProperty="(Canvas.Left)"
                                                        To="3.5"
                                                        Duration="0:0:0.16" />
                                    <ColorAnimation x:Name="ButtonStyleUnChecked_ColorAnimation" Storyboard.TargetName="TeslaToggle_Rectangle"
                                                    Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)"
                                                    To="#DBDBDB"
                                                    Duration="0:0:0.16" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="DesignToggle" x:Key="SimpleToggleButton" TargetType="{x:Type CheckBox}">
        <Setter x:Name="DesignToggle_Template" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="DesignToggle_ControlTemplate" TargetType="{x:Type CheckBox}">
                    <Viewbox x:Name="DesignToggle_ViewBox">
                        <Border x:Name="DesignToggle_Border" CornerRadius="10"
                                Background="#FFE2E2E2"
                                Width="40" Height="20">
                            <Border.Effect>
                                <DropShadowEffect x:Name="DesignToggle_DropShaddow" ShadowDepth="0.5" Direction="0" Opacity="0.3" />
                            </Border.Effect>
                            <Ellipse x:Name="DesignToggle_Ellipse" Fill="#FF909090" Stretch="Uniform"
                                 Margin="-8 -4"
                                 Stroke="Gray" StrokeThickness="0.2"
                                 HorizontalAlignment="Stretch">
                                <Ellipse.Effect>
                                    <DropShadowEffect x:Name="DesignToggleEllipse_DropShaddow" BlurRadius="10" ShadowDepth="1" 
                                                  Opacity="0.3" Direction="260" />
                                </Ellipse.Effect>
                            </Ellipse>
                        </Border>
                    </Viewbox>
                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="DesignToggleChecked_EventTrigger" RoutedEvent="Checked">
                            <BeginStoryboard x:Name="DesignToggleChecked_BeginStoryBoard">
                                <Storyboard x:Name="DesignToggleChecked_StoryBoard">
                                    <ColorAnimation x:Name="DesignToggleChecked_ColorAnimation" Storyboard.TargetName="DesignToggle_Ellipse"
                                                    Storyboard.TargetProperty="(Ellipse.Fill).(SolidColorBrush.Color)"
                                                    To="#FF0069F3"
                                                    Duration="0:0:0.05"
                                                    AccelerationRatio="0.7"
                                                    DecelerationRatio="0.3"/>
                                    <ThicknessAnimation x:Name="DesignToggleChecked_ThicknessAnimation" Storyboard.TargetName="DesignToggle_Ellipse"
                                                    Storyboard.TargetProperty="Margin"
                                                    To="20 -4 -8 -4"
                                                    Duration="0:0:0.15" 
                                                    AccelerationRatio="0.7"
                                                    DecelerationRatio="0.3"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger x:Name="DesignToggleUnChecked_EventTrigger" RoutedEvent="Unchecked">
                            <BeginStoryboard x:Name="DesignToggleUnChecked_BeginStooryBoard">
                                <Storyboard x:Name="DesignToggleUnChecked_StoryBoard">
                                    <ColorAnimation x:Name="DesignToggleUnChecked_ColorAnimation" Storyboard.TargetName="DesignToggle_Ellipse"
                                                    Storyboard.TargetProperty="(Ellipse.Fill).(SolidColorBrush.Color)"
                                                    To="#FF909090"
                                                    Duration="0:0:0.05" 
                                                    AccelerationRatio="0.7"
                                                    DecelerationRatio="0.3"/>
                                    <ThicknessAnimation x:Name="DesignToggleUnChecked_ThicknessAnimation" Storyboard.TargetName="DesignToggle_Ellipse"
                                                    Storyboard.TargetProperty="Margin"
                                                    To="-8 -4"
                                                    Duration="0:0:0.15"
                                                    AccelerationRatio="0.7"
                                                    DecelerationRatio="0.3"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="TeslaPanelToggleButton" x:Key="TeslaPanelToggleButton"
           BasedOn="{StaticResource {x:Type ToggleButton}}"
           TargetType="{x:Type RadioButton}">
        <Setter x:Name="TeslaPanelButton_Light_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="TeslaPanelButton_Light_ControlTemplate"
                                 TargetType="{x:Type RadioButton}">
                    <Viewbox x:Name="TeslaPanelButton_Light_ViewBox">
                        <Grid x:Name="TeslaPanelButton_Light_Border" Background="Transparent" Width="191" Height="45">
                            <TextBlock Text="{TemplateBinding Property=Content}"
                                       VerticalAlignment="Center"
                                       FontFamily="/Fonts/#Montserrat"
                                       Margin="120,0,0,0"
                                       />
                        </Grid>
                    </Viewbox>
                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="TeslaPanelButton_Light_Checked_EventTrigger" RoutedEvent="Checked">
                            <BeginStoryboard x:Name="TeslaPanelButton_Light_Checked_BeginStoryboard">
                                <Storyboard x:Name="TeslaPanelButton_Light_Checked_StoryBoard">
                                    <ColorAnimation x:Name="TeslaPanelButton_Light_ColorAnimation" Storyboard.TargetName="TeslaPanelButton_Light_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="#4C0079FF"
                                                    Duration="0:0:00.35" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger x:Name="TeslaPanelButton_Light_EventTrigger" RoutedEvent="Unchecked">
                            <BeginStoryboard x:Name="TeslaPanelButton_Light_UnChecked_BeginStoryBoard">
                                <Storyboard x:Name="TeslaPanelButton_Light_UnChecked_StoryBoard">
                                    <ColorAnimation x:Name="TeslaPanelButton_Light_UnChecked_ColorAnimation" Storyboard.TargetName="TeslaPanelButton_Light_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="Transparent"
                                                    Duration="0:0:00.35" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="HiddenButton" x:Key="HiddenButton"
           TargetType="{x:Type Button}">
        <Setter x:Name="HiddenButton_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="HiddenButton_ControlTemplate" TargetType="{x:Type Button}">
                    <Border x:Name="HiddenButton_Border" CornerRadius="6" Background="#02DA4040" Width="36" Height="36"/>
                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="HiddenButton_Checked_EventTrigger" RoutedEvent="MouseEnter">
                            <BeginStoryboard x:Name="HiddenButton_Checked_BeginStoryboard">
                                <Storyboard x:Name="HiddenButton_Checked_StoryBoard">
                                    <ColorAnimation x:Name="HiddenButton_ColorAnimation" Storyboard.TargetName="HiddenButton_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="#4C0079FF"
                                                    Duration="0:0:00.35" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger x:Name="HiddenButton_EventTrigger" RoutedEvent="MouseLeave">
                            <BeginStoryboard x:Name="HiddenButton_UnChecked_BeginStoryBoard">
                                <Storyboard x:Name="HiddenButton_UnChecked_StoryBoard">
                                    <ColorAnimation x:Name="HiddenButton_UnChecked_ColorAnimation" Storyboard.TargetName="HiddenButton_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="Transparent"
                                                    Duration="0:0:00.35" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="TeslaButton" x:Key="TeslaButton" TargetType="{x:Type ToggleButton}">
        <Setter x:Name="TeslaButton_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="TeslaButton_ControlTemplate" TargetType="{x:Type ToggleButton}">
                    <Viewbox x:Name="TeslaButton_ViewBox">
                        <Border BorderThickness="0" x:Name="TeslaButton_Border" CornerRadius="10" Background="White" Width="75" Height="20">
                            <Border.Effect>
                                <DropShadowEffect x:Name="TeslaButton_DropShaddow"  Color="#FFE0E0E0" Direction="676" ShadowDepth="1" BlurRadius="2"/>
                            </Border.Effect>
                        </Border>
                    </Viewbox>
                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="TeslaButtonChecked_EventTrigger" RoutedEvent="Checked">
                            <BeginStoryboard x:Name="TeslaButtonkChecked_BeginStoryboard">
                                <Storyboard x:Name="TeslaButtonChecked_StoryBoard">
                                    <ColorAnimation x:Name="TeslaButtonChecked_ColorAnimation" Storyboard.TargetName="TeslaButton_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="DodgerBlue"
                                                    
                                                    Duration="0:0:0.1" />
                                    <ThicknessAnimation x:Name="TeslaButtonChecked_ThicknessAnimation" Storyboard.TargetName="TeslaButton_Border"
                                                        Storyboard.TargetProperty="Margin"
                                                        To=".2 .2 .2 .2"
                                                        Duration="0:0:0.1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger x:Name="TeslaButtonUnChecked_EventTrigger" RoutedEvent="Unchecked">
                            <BeginStoryboard x:Name="TeslaButtonUnChecked_BeginStoryBoard">
                                <Storyboard x:Name="TeslaButtonUnChecked_StoryBoard">
                                    <ColorAnimation x:Name="TeslaButtonUnChecked_ColorAnimation" Storyboard.TargetName="TeslaButton_Border"
                                                    Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"
                                                    To="White"
                                                    Duration="0:0:0.1" />
                                    <ThicknessAnimation x:Name="TeslaButtonUnChecked_ThicknessAnimation" Storyboard.TargetName="TeslaButton_Border"
                                                        Storyboard.TargetProperty="Margin"
                                                        To="0 0 0 0"
                                                        Duration="0:0:0.1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="SafetyData_Button" x:Key="SafetyData_Button" TargetType="{x:Type Button}">
        <Setter x:Name="SafetyData_Button_Light_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="SafetyData_Button_Light_ControlTemplate" TargetType="{x:Type Button}">
                    <Viewbox x:Name="SafetyData_Button_Light_ViewBox">
                        <Border x:Name="SafetyData_Button_Light_Border" CornerRadius="10" Height="261" Width="356">
                            <Border.Background>
                                <ImageBrush Stretch="Fill" ImageSource="/Tesla Demo;component/Resources/safelight.png" />
                            </Border.Background>
                        </Border>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="ImageNotif_Button" x:Key="ImageNotif_Button" TargetType="{x:Type Button}">
        <Setter x:Name="ImageNotif_Button_Light_Setter" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Name="ImageNotif_Button_Light_ControlTemplate" TargetType="{x:Type Button}">
                    <Viewbox x:Name="ImageNotif_Button_Light_ViewBox">
                        <Border x:Name="ImageNotif_Button_Light_Border" CornerRadius="56" Height="101" Width="363">
                            <Border.Background>
                                <ImageBrush Stretch="UniformToFill" ImageSource="/Tesla Demo;component/Resources/lightwidetesla.png" />
                            </Border.Background>
                        </Border>
