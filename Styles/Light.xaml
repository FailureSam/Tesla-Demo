<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- primary -->
    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#1e90ff"/>
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="DodgerBlue"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="DodgerBlue"/>
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF"/>
    <!-- accent -->
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="DodgerBlue"/>
    <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#FFFFFF"/>

    <Style x:Name="ThemeControl_Invert_Rectangle" x:Key="ThemeControl_Invert_Rectangle" TargetType="Rectangle">
        <Setter Property="Fill" Value="Black"/>
        <Setter Property="Stroke" Value="Black"/>
    </Style>

    <Style x:Name="ThemeControl_Invert_TextBlock" x:Key="ThemeControl_Invert_TextBlock" TargetType="TextBlock">
        <Setter Property="Foreground" Value="Black"/>
    </Style>

    <Style x:Name="ThemeControl_Invert_TextBox" x:Key="ThemeControl_Invert_TextBox" TargetType="TextBox">
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="Background" Value="Transparent"/>
    </Style>

    <Style x:Name="ThemeControl_Invert_LoginTextBlock" x:Key="ThemeControl_Invert_LoginTextBlock" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#FF555555"/>
    </Style>

    <Style x:Name="ThemeControl_Slider_Border" x:Key="ThemeControl_Slider_Border" TargetType="Border">
        <Setter Property="Background" Value="White"/>
        <Setter Property="Rectangle.Effect">
            <Setter.Value>
                <DropShadowEffect Color="#FFE0E0E0" Direction="676" ShadowDepth="2"/>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Name="ThemeControl_Element_Rectangle" x:Key="ThemeControl_Element_Rectangle" TargetType="Rectangle">
        <Setter Property="Fill" Value="#DBDBDB"/>
    </Style>

    <Style x:Name="ThemeControl_PopUp_Element_Rectangle" x:Key="ThemeControl_PopUp_Element_Rectangle" TargetType="Rectangle">
        <Setter Property="Fill" Value="White"/>
    </Style>

    <Style x:Name="ThemeControl_Grid" x:Key="ThemeControl_Grid" TargetType="Grid">
        <Setter Property="Background" Value="#FFEBEBEB"/>
    </Style>

    <Style x:Name="ThemeControl_Border" x:Key="ThemeControl_Border" TargetType="Border">
        <Setter Property="BorderBrush" Value="#FFEBEBEB"/>
    </Style>

    <Style x:Name="ThemeControl_Card_Border" x:Key="ThemeControl_Card_Border" TargetType="Border">
        <Setter Property="Background" Value="#FFD9D9D9"/>
    </Style>

    <Style x:Name="ThemeControl_StatusBar_Border" x:Key="ThemeControl_StatusBar_Border" TargetType="Border">
        <Setter Property="Background" Value="WhiteSmoke"/>
    </Style>

    <Style x:Name="ThemeControl_StatusBar_StackPanel" x:Key="ThemeControl_StatusBar_StackPanel" TargetType="StackPanel">
        <Setter Property="Background" Value="WhiteSmoke"/>
    </Style>

    <Style x:Name="ThemeControl_Invert_Path" x:Key="ThemeControl_Invert_Path" TargetType="{x:Type Path}">
        <Setter Property="Path.Fill" Value="Black" />
        <Setter Property="Path.Stroke" Value="Black"/>
    </Style>

    <Style x:Name="ThemeControl_Invert_Tesla_Path" x:Key="ThemeControl_Invert_Tesla_Path" TargetType="{x:Type Path}">
        <Setter Property="Path.Fill" Value="#FF353535" />
        <Setter Property="Path.Stroke" Value="Black" />
    </Style>

    <Style x:Name="ThemeControl_Invert_Text" x:Key="ThemeControl_Invert_Text" TargetType="{x:Type TextBlock}">
        <Setter Property="Path.Fill" Value="Black" />
        <Setter Property="Path.Stroke" Value="Black"/>
    </Style>

    <Style x:Name="TeslaPanel" x:Key="TeslaPanel" TargetType="{x:Type StackPanel}">
        <Setter Property="Background" Value="#A5FFFFFF"/>
    </Style>

