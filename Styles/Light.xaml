                    <ControlTemplate.Triggers>
                        <EventTrigger x:Name="TeslaStyle_ToggleChecked_EventTrigger" RoutedEvent="Checked">
                            <BeginStoryboard x:Name="TeslaStyle_ToggleChecked_BeginStoryBoard">
                                <Storyboard x:Name="TeslaStyle_ToggleChecked_StoryBoard">
                                    <DoubleAnimation x:Name="TeslaStyleChecked" Storyboard.TargetName="TeslaStyle_Light_BallSlide"
                                                        Storyboard.TargetProperty="RenderTransform.X"
                                                        To="35"
                                                        Duration="0:0:0.3" >
                                        <DoubleAnimation.EasingFunction>
                                            <CubicEase EasingMode="EaseIn"/>
                                        </DoubleAnimation.EasingFunction>
                                    </DoubleAnimation>
